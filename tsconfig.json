{
    "compilerOptions": {
        "outDir": "./static/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "es6",
        "target": "esnext",
        "jsx": "react",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true
    },
    "include": [
        "./src/**/*"
    ]
}
